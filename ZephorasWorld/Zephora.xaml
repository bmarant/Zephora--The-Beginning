<Window x:Class="ZephorasWorld.Zephora"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Zephora" Height="437" Width="695" Background="White" ResizeMode="NoResize" Icon="Splash.png" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,2,2">
        <Grid.Background>
            <ImageBrush ImageSource="08-Wallpaper.png"/>
        </Grid.Background>
        <Button Content="Create Character" HorizontalAlignment="Left" Margin="288,193,0,0" VerticalAlignment="Top" Width="122" Height="46" Click="Button_Click" Foreground="#FF1B4FFF" FontFamily="Segoe UI Black" BorderThickness="0">
            <Button.Background>
                <ImageBrush ImageSource="button.png"/>
            </Button.Background>
        </Button>
        <Button Content="Load Character" HorizontalAlignment="Left" Margin="288,107,0,0" VerticalAlignment="Top" Width="122" Height="46" Click="Button_Click_1" Foreground="#FF1B4FFF" FontFamily="Segoe UI Black" FontWeight="Bold" BorderThickness="0">
            <Button.Background>
                <ImageBrush ImageSource="button.png"/>
            </Button.Background>
        </Button>
    </Grid>
</Window>
